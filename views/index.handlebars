			<center>
                <!------ LOGIN -------->
                <button type="button" id="outline" class="btn btn-Login-outline" data-toggle="modal" data-target="#login-modal">&nbsp;&nbsp;&nbsp;&nbsp;Login&nbsp;&nbsp;&nbsp;&nbsp;</button>
                
                <!------ LOGIN MODAL -------->
                <div class="modal fade" id="login-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
    	  <div class="modal-dialog">
				<div class="loginmodal-container" id="login">
					<h1>Welcome!</h1><br>
				  <form role="form" method="get">
					<input type="text" name="user" placeholder="Username" id="username">
					<input type="password" name="pass" placeholder="Password" id="password">
					<input id="submitLogin" name="login" class="login loginmodal-submit login" value="Login" method="get" type="button" onclick="window.location='/sample';">
				  </form>
					
				  <div class="login-help">
					<a href="#">Register</a> - <a href="#">Forgot Password</a>
				  </div>
				</div>
			</div>
		  </div>
                <!------ Sign Up -------->
                <button type="button" class="btn btn-SignUp-outline" id="outline1" data-toggle="modal" data-target="#signup-modal">&nbsp;&nbsp;Sign Up&nbsp;&nbsp;</button>
                
                <!------ SignUp MODAL -------->
                <div class="modal fade" id="signup-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
    	  <div class="modal-dialog">
				<div class="loginmodal-container" id="register">
					<h1>Welcome!</h1><br>
				  <form role="form" method="post">
					<input type="text" name="user" placeholder="Username" id="usernamePick">
                    <input type="text" name="email" placeholder="Email" id="emailPick"> 
					<input type="password" name="pass" placeholder="Password!" id="passwordPick">
                    <input type="password" name="pass" placeholder="Confirm Password">  
					<input id="submitLogin" name="signup" class="login loginmodal-submit register" value="Create Account" method="post" type="button" onclick="window.location='/sample'; ga('send', 'event', 'User Entry', 'Login');">
				  </form>
					
				</div>
			</div>
		  </div>
                
                <!------ Browse as guest -------->
                <p style="padding: 10px; color: white; font-size: 12pt;" onclick="window.location='./sample';">Browse as guest . . .</p>
			</div>
			</center>
